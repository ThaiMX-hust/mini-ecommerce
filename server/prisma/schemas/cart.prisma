model Cart {
    cart_id     String   @id @default(uuid())
    user_id     String
    total_items Int
    total_price Decimal
    created_at  DateTime @default(now())
    updated_at  DateTime @updatedAt

    user  User        @relation(fields: [user_id], references: [user_id], onDelete: Cascade)
    items CartItems[]
}
